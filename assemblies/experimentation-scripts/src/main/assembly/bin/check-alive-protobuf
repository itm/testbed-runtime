#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 2 ]
then
	echo "Usage $0 PROPERTIES_FILE SECRET_RESERVATION_KEYS [NODEURNS]"
	echo "Example: $0 live.properties urn:wisebed:uzl1:,D2C88AEE416EDD2FD4038BC1252A8900 urn:wisebed:uzl1:0xf859,urn:wisebed:uzl1:0xf860"
	exit 1
fi

java -Dtestbed.secretreservationkeys=$2 -Dtestbed.nodeurns=$3 -jar $BASEDIR/../lib/tr.scripting-client-${project.version}-onejar.jar -p $1 -f $BASEDIR/../scripts/check-alive-protobuf.java
